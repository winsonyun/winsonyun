.vote-activity-item-view{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 375rpx;
  height: 476rpx;
  float: left;
}

.activity-img{
  width: 330rpx;
  height: 300rpx;
}
.activity-img image{
  width: 100%;
  height: 100%;
}

.activity-name text{
  font-size: 25rpx;
  font-weight: 20rpx;
  color: #019898;
}

.activity-time text{
  font-size: 25rpx;
  font-weight: 20rpx;
  color: #019898;
}

.activity-things text{
  margin: 10rpx;
  font-size: 25rpx;
  font-weight: 20rpx;
  color: #019898;
}



