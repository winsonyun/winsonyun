      <view class="lfy_ph_number">{{voteNum}}</view>
      <view class="lfy_4touxiang_img">
        <image class="lfy_4imghigh1" src="{{works_img}}"></image>
      </view>
      <text class="lfy_4name1">{{works_name}}</text>
